<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.travellet">


    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat">
        <activity android:name=".InputCostActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddPlanActivity" />
        <activity android:name=".InputBudgetActivity" />
        <activity android:name=".PlaceDetailActivity">

        </activity>
        <activity android:name=".SetBudgetActivity" />
        <activity android:name=".SetDateActivity" />
        <activity android:name=".SetTitleActivity" />
        <activity android:name=".SignUp2Activity" />
        <activity android:name=".SignUp1Activity" />
        <activity android:name=".SignInActivity" />
        <activity android:name=".MainActivity">
        </activity>
    </application>

</manifest>